@Article{TherMec,
  author = 	 {Tage Christensen and Bo Jakobsen and Jon Papini and Tina Hecksher  and Jeppe C. Dyre and Niels Boye Olsen},
  title = 	 {A Combined Measurement of Thermal and Mechanical Relaxation.},
  journal = 	 {Journal of Non-Crystalline Solids},
  year = 	 2011,
  volume = 	 357,
  pages = 	 {346-350},
  note = 	 {``6th IDMRCS'' special issue},
  doi = {10.1016/j.jnoncrysol.2010.07.051},
  OLDurl= {http://arxiv.org/abs/1004.2651},
  reprint = {2011/Christensen_et_al_JNCS_2011_vol357_p346.pdf},
  abstract={In order to describe relaxation the thermodynamic
                  coefficient $\frac{1} {\beta_s}=\left(\frac{\tial
                  V}{\tial S} \right)_p$ can be generalized into a
                  complex frequency-dependent cross response
                  function. We explore theoretically the possibility
                  of measuring $\frac{1}{\beta_S}$ for a supercooled
                  liquid near the glass transition. This is done by
                  placing a thermistor in the middle of the liquid
                  which itself is contained in a spherical
                  piezoelectric shell. The piezoelectric voltage
                  response to a thermal power generated in the
                  thermistor is found to be proportional to $\frac{1}{\beta_S}$
                  but factors pertaining to heat diffusion and
                  adiabatic compressibility $\kappa_S(\omega)$ do also
                  intervene. Weestimate a measurable piezoelectric
                  voltage of 1 mV to be generated at 1 Hz for a heating
                  power of 0.3mW. Together with $\kappa_S(\omega)$ and the
                  longitudinal specific heat $c_l(\omega)$ which may also be
                  found in the same setup a complete triple of
                  thermoviscoelastic response functions may be
                  determined when supplemented with shear modulus
                  data.  },
  pubtype={major}
}

@Article{BetaShear,
  author = 	 {Bo Jakobsen and Kristine Niss and Claudio Maggi and Niels Boye Olsen and Tage Christensen and Jeppe C. Dyre},
  title = 	 {Beta relaxation in the shear mechanics of viscous liquids: Phenomenology and network modeling of the alpha-beta merging region},
  journal = 	 {Journal of Non-Crystalline Solids},
  year = 	 2011,
  volume = 	 357,
  pages = 	 {267--273},
  note = 	 {``6th IDMRCS'' special issue},
  doi={10.1016/j.jnoncrysol.2010.08.010},
  Oldurl= {http://arxiv.org/abs/1004.2651},
  reprint = {2011/Jakobsen_et_al_JNCS_2011_vol357_p267.pdf},
abstract={The phenomenology of the beta relaxation process in the
                  shear-mechanical response of glass-forming liquids
                  is summarized and comed to that of the dielectric
                  beta process. Furthermore, we discuss how to model
                  the observations by means of standard viscoelastic
                  modeling elements. Necessary physical requirements
                  to such a model are outlined, and it is argued that
                  physically relevant models must be additive in the
                  shear compliance of the alpha and beta ts. A
                  model based on these considerations is proposed and
                  fitted to data for Polyisobutylene 680.},
  pubtype={major}
}

