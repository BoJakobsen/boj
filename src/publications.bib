@Article{Noirat2024_GlyWa,
author ="Noirat, David B. and Frick, Bernhard and Jakobsen, Bo and Appel, Markus and Niss, Kristine",
title  ="Density scaling and isodynes in glycerol-water mixtures",
journal  ="Phys. Chem. Chem. Phys.",
year  ="2024",
volume  ="26",
issue  ="46",
pages  ="29003-29014",
publisher  ="The Royal Society of Chemistry",
doi  = "10.1039/D4CP02231A",
abstract  ="This paper presents dielectric and neutron spectroscopy data on two different glycerol-water mixtures at elevated pressures. Glycerol-water liquid mixtures have a high concentration of hydrogen bonds which usually is expected to lead to complex dynamics. However{,} with regard to the pressure dependence of the dynamics we reveal a surprisingly simple picture. Different aspects of the dynamics have the same pressure dependence{,} in other words the phase diagram of the liquids have so-called isodynes{,} density scaling is also observed to hold reasonably well and there is even some reminiscence of isochronal superposition. This suggests that these aspect of liquid dynamics are very general and hold for different types of intermolecular interactions.",
reprint = {2024/Noirat_et_al_PCCP_vol26_p29003.pdf},
pubtype={major},
}


