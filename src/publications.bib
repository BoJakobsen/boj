@Article{GammaGamma,
  author = 	 {Ditte Gundermann and Ulf R. Pedersen and Tina Hecksher and Nicholas P. Bailey and Bo Jakobsen and Tage Christensen and Niels B. Olsen and Thomas B. Schrøder and Daniel Fragiadakis and Riccardo Casalini and C. Michael Roland and Jeppe C. Dyre and Kristine Niss },
  title = 	 {Predicting the density scaling exponent from Prigogine-Defay ratio measurements},
  journal = 	 {Nature Physics},
  year = 	 2011,
  volume = 	 {7},
  pages = 	 {816--821},
  doi = {doi:10.1038/nphys2031},
  OPTurl= {http://arxiv.org/abs/1106.0227},
  reprint = {2011/Gunderman_et_al_NatPhys_2011.pdf},
  abstract={Understanding the origin of the dramatic temperature and
                  density dependence of the relaxation time of
                  glass-forming liquids is a fundamental challenge in
                  glass science. The recently established
                  "density-scaling" relation quantifies the relative
                  importance of temperature and density for the
                  relaxation time in terms of a material-dependent
                  exponent. We show that this exponent for approximate
                  single-ameter liquids can be calculated from
                  thermoviscoelastic linear-response data at a single
                  state point, for instance an ambient-pressure state
                  point. This prediction is confirmed for the van der
                  Waals liquid
                  tetramethyl-tetraphenyl-trisiloxane. Consistent with
                  this, a compilation of literature data for the
                  Prigogine-Defay ratio shows that van derWaals
                  liquids and polymers are approximate
                  single-ameter systems, whereas associated and
                  network-forming liquids are not.},
  pubtype={major}
}


